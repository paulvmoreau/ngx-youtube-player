{
  "name": "ngx-youtube-player-14",
  "version": "7.1.14",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:lib": "ng build ngx-youtube-player-14",
    "dev:lib": "ng build ngx-youtube-player-14 --watch",
    "test": "ng test",
    "test:lib": "ng test ngx-youtube-player-14",
    "test:lib:ci": "CI=true ng test ngx-youtube-player-14",
    "publish:lib": "npm publish ./dist/ngx-youtube-player-14",
    "publish:lib:next": "npm publish --tag next ./dist/ngx-youtube-player-14",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:package": "yarn build && cd ./dist/ngx-youtube-player-14 && yarn pack",
    "prelib:publish": "yarn lib:package",
    "lib:publish": "cd ./dist/ngx-youtube-player-14 && yarn publish --access=public",
    "tags": "yarn tag list @paulvmoreau/ngx-youtube-player-14"
  },
